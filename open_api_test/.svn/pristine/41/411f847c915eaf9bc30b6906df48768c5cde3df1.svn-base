package com.xy.api.demo.params;

import lombok.Data;

/**
 * 审核通知
 */
@Data
public class CheckNoticeBo {
    /**
     * 业务编码
     */
    private String xyCode;
    /**
     * 版本
     */
    private String xyVersion;
    /**
     * 状态
     */
    private int status;
    /**
     * 状态描述
     */
    private String statusStr;
    /**
     * 审核备注
     */
    private String remark;
    /**
     *
     */
    private String dataType;


    public CheckNoticeBo(final String xyCode, final String xyVersion, final int status, final String statusStr, final String remark, final String dataType) {
        this.xyCode = xyCode;
        this.xyVersion = xyVersion;
        this.status = status;
        this.statusStr = statusStr;
        this.remark = remark;
        this.dataType = dataType;
    }

    public CheckNoticeBo() {
    }
}